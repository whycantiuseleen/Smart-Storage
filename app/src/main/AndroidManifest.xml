<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eileen.smartstorage">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ReturnTools" />
        <activity android:name=".ChooseItem" />
        <activity android:name=".BookTool" />
        <activity android:name=".HistoryMain" />
        <activity android:name=".Profile" />
        <activity android:name=".MainAdmin" />
        <activity android:name=".MakeReport" />
        <activity android:name=".AdminHistoryMain"></activity>
    </application>

</manifest>